        "app:install": "npm install --workspace=@proton/frontend",
		"app:preview": "npm run preview --workspace=@proton/frontend",
		"wasm:build": "npm run build --workspace=@proton/wasm",
		"wasm:test": "npm run test --workspace=@proton/wasm"
